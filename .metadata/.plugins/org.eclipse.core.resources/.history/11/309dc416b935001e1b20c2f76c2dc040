package ArrayPrograms;

public class MaxNumber {
	
	public static void main(String[] args) {
		
	
	int arr[]= {1,2,3,2,5,6,7,8,9};
	
	int max = arr[0] ;
	
	for(int i=0;i<arr.length-1;i++)
	{
		for(int j=i+1;j<arr.length;j++)
		{
			if(arr[i]>arr[j])
			{
				max=arr[i];
			}
		}
		
		
	}
	System.out.println(max);
	
		
	}	

}
